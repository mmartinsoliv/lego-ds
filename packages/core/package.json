{
  "name": "@lego-ds/core",
  "version": "1.0.0",
  "description": "Lego-ds Core Component Library",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/lego-ds/core/lego-ds.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "outputTargets": [
    {
      "type": "dist",
      "esmLoaderPath": "../loader"
    }
  ],
  "publishConfig": {
    "access": "public",
    "directory": ["dist", "loader"]
  },
  "scripts": {
    "prebuild": "rm -rf dist && rm -rf loader && rm -rf www",
    "build": "stencil build --prod",
    "dev": "stencil build --prod --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "dependencies": {
    "@stencil/react-output-target": "^0.0.6"
  },
  "devDependencies": {
    "@stencil/core": "^1.1.0",
    "@stencil/sass": "^1.3.1",
    "@types/node": "^11.11.8"
  },
  "author": "Victor Caetano <victorsax12@gmail.com>",
  "homepage": "https://github.com/victormath12/lego-ds#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/victormath12/lego-ds.git"
  },
  "bugs": {
    "url": "https://github.com/victormath12/lego-ds/issues"
  },
  "gitHead": "152be256457a023876fa42e98de9a7d2f34e5bfa"
}
