{
  "name": "@lego-ds/core",
  "version": "0.1.0",
  "description": "Lego-ds Core Component Library",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/lego-ds/core/lego-ds.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "outputTargets": [
    {
      "type": "dist",
      "esmLoaderPath": "../loader"
    }
  ],
  "scripts": {
    "build": "stencil build --prod",
    "dev": "stencil build --prod --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "dependencies": {
    "@stencil/react-output-target": "^0.0.6"
  },
  "devDependencies": {
    "@stencil/core": "^1.1.0",
    "@stencil/sass": "^1.3.1",
    "@types/node": "^11.11.8"
  },
  "license": "MIT"
}
